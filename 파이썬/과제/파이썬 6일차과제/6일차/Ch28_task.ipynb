   "source": [
    "def min_max(tuple_num):\n",
    "    print(min(tuple_num))\n",
    "    print(max(tuple_num))\n",
    "\n",
    "tuple_num=tuple(input('3개의 숫자 입력(,)').split(','))\n",
    "min_max(tuple_num)"
   ]
